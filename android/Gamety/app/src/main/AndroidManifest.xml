<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mahmoudreda.gamety">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sign_up" />
        <activity android:name=".sign_in" />
        <activity
            android:name=".schedule"
            android:label="@string/title_activity_schedule" />
        <activity
            android:name=".ADV_dash_board"
            android:label="@string/title_activity_adv_dash_board" />
        <activity
            android:name=".Courses_content"
            android:label="@string/title_activity_courses_content" />
        <activity
            android:name=".notification"
            android:label="@string/title_activity_notification"></activity>
    </application>

</manifest>